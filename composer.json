{
	"name": "rivervanrain/market",
	"type": "elgg-plugin",
	"description": "Elgg marketplace for creating classifieds",
	"license": "GPL-2.0-only",
	"keywords": [
		"elgg", "plugin", "commerce", "classifieds", "marketplace"
	],
	"authors": [
		{
		  "name": "Nikolai Shcherbin",
		  "email": "support@wzm.me"
		}
	],
	"require": {
		"php": "^7.4|^8.0",
		"composer/installers": "^1.0.8",
		"npm-asset/dropzone": "~5.0"
	},
	"autoload": {
		"psr-0": {
			"": "classes/"
		}
	},
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
	"conflict": {
		"elgg/elgg": "<4.3"
	},
	"support": {
		"source": "https://github.com/rivervanrain/market"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}
